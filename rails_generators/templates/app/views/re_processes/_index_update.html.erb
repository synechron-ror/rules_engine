<% if @re_processes.nil? || @re_processes["processes"].nil? || @re_processes["processes"].empty? %>
	<h6 class="left-50 red"><em>Not Processed</em></h6>
<% else %>
	<% @re_processes["processes"].each do |re_process| %>
		<div class="clear top-2"></div>
		<div class="left-10 right-5">
			<div class="re-code-small float-left re-xtra-process-<%= re_process_status(re_process["process_status"]) %>">	
				<%= re_process["plan_code"] %> <span class="blue">V.<%= re_process["plan_version"] %></span>
			</div>	
			<div class="smalltext float-right right-5">	
				<%= re_process["started_at"].nil? ? '' : Time.parse(re_process["started_at"]).strftime('%d %b %Y - %H:%M') %>
			</div>	
			<hr/>	
		</div>		
	<% end %>		
	<% if !@re_processes["processes"].empty? && (@re_processes["next_page"] || @re_processes["previous_page"]) %>
		<a class="re-xtra-process-next-<%= @re_processes["next_page"] ? 'enabled' : 'disabled' %>" href='#<%= @re_processes["next_page"] || '1' %>'></a>
		<a class="re-xtra-process-prev-<%= @re_processes["previous_page"] ? 'enabled' : 'disabled' %>" href='#<%= @re_processes["previous_page"] || '1' %>'></a>
	<% end %>
<% end %>