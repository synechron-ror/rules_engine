$.fancybox.close();
$.re_unblock();

<%= re_alert_js %>

$('#re_plan_change').html('<%=escape_javascript(render('/re_plans/change'))%>');

$('#re_workflow_change').html('<%=escape_javascript(render('/re_plans/workflow_empty', :re_plan => @re_plan))%>');
<% @re_plan.re_workflows.each do |re_workflow | %>
  $('#re_workflow_change').append('<%=escape_javascript(render('/re_plans/workflow_change', :re_workflow => re_workflow))%>');
<% end %>

$('#re_plan_menu').html('<%=escape_javascript(render('/re_plans/menu'))%>');
