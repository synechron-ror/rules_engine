<% set_re_javascript_include ['re_pipeline_index', 're_pipeline_new'] %>
<% set_re_subnav 'All Pipelines', 
								link_to("New Pipeline", new_re_pipeline_path, :class => "re-pipeline-new"),
								link_to("All Jobs", re_jobs_path, :class => "re-pipeline-list") %>

<div class="prepend-1 span-22 append-1">
  <div class="clear" id="re_pipeline_index"></div>
  
  <%= render 'empty', :hide => !@re_pipelines.empty? %>

  <% @re_pipelines.each do | re_pipeline | %>
    <%= render 'index', :re_pipeline => re_pipeline %>
  <% end %>  

</div>  
<div class="clear"></div>