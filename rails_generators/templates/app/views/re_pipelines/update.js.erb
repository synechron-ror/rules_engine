$.fancybox.close();
$.re_unblock();

<%= re_alert_js %>

$('#re_pipeline_change').html('<%=escape_javascript(render('/re_pipelines/change'))%>');
$('#re_pipeline_change_actions').html('<%=escape_javascript(render('/re_pipelines/change_actions'))%>');

<%= render('/re_rules/empty', :re_pipeline => @re_pipeline) %>

$('#re_rules_change').html('')
<% @re_pipeline.re_rules.each do |re_rule | %>
  $('#re_rules_change').append('<%=escape_javascript(render('/re_rules/change', :re_rule => re_rule, :first_rule => re_rule == @re_pipeline.re_rules[0], :last_rule => re_rule == @re_pipeline.re_rules[-1]))%>');
<% end %>
